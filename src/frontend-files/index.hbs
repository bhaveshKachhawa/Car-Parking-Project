<html>

<head>
    {{>headLinks}}
</head>

<body
    style="background:url('https://images.unsplash.com/photo-1495435229349-e86db7bfa013?q=80&w=1377&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D');background-size:cover;background-position:center;">
    {{>homeNavBar}}
    {{#if message}}
    <div class="position-absolute alert alert-primary show mt-5" role="alert">
        <strong>{{message}}</strong>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    {{/if}}
    <section class="d-flex align-items-center h-75 pt-5 mt-5">
        <div class="container">
            <div class="row d-flex align-items-center">
                <div class="col-lg-6 d-flex flex-column align-items-center gap-2 pt-4">
                    {{>buttons}}
                </div>
                <div class="col-lg-6 d-flex flex-column align-items-center gap-2 pt-4">
                    {{>displaySlotsInformation}}
                </div>
            </div>
        </div>
    </section>
    {{>raiseATicketModel}}
    {{>getRegisterNumberModel}}
    {{>getSlotNumberModel}}
    {{>getSlotNumbersModel}}
    {{>bootstrapLinks}}
    {{>removeCarModel}}
    {{>removedCarsModel}}
    {{>footer}}
    {{>myJavaScriptLinks}}
</body>

</html>